{
  "properties": [
    {
      "name": "minio.endpoint",
      "type": "java.lang.String",
      "description": "endPoint是一个URL，域名，IPv4或者IPv6地址."
    },
    {
      "name": "minio.accessKey",
      "type": "java.lang.String",
      "description": "accessKey类似于用户ID，用于唯一标识你的账户."
    },
    {
      "name": "minio.secretKey",
      "type": "java.lang.String",
      "description": "secretKey是你账户的密码."
    },
    {
      "name": "minio.secure",
      "type": "java.lang.Boolean",
      "description": "如果是true，则用的是https而不是http,默认值是true."
    },
    {
      "name": "minio.bucketName",
      "type": "java.lang.String",
      "description": "默认存储桶."
    },
    {
      "name": "minio.configDir",
      "type": "java.lang.String",
      "description": "配置目录."
    }
  ] }